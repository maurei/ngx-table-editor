  "scripts": {
    "build": "node build.js",
    "test": "karma start",
    "test:watch": "karma start karma.conf.js --single-run false",
    "pack:lib": "npm run build &&  mv $(npm pack ./dist) package.tgz && mv package.tgz ../core.tgz",
    "publish:lib": "npm run build && npm publish ./dist",
    "publish:lib:next": "npm run build && npm publish --tag next ./dist",
    "compodoc": "compodoc --disableInternal -p tsconfig.json -s --theme material --hideGenerator --output ../project-page/documentation"
